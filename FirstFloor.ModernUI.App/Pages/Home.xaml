                <mui:Link  DisplayName="Inventario" Source="/Pages/Tabs/Inventario.xaml" />
                <mui:Link DisplayName="Reportes" Source="/Pages/TransLoginToReporte.xaml" />
            </mui:ModernTab.Links>
